{
  "name": "goat-minify",
  "version": "2.4.3",
  "description": "Client-side code minification tool supporting JS, CSS, HTML, XML, JSON, YAML, TOML, and Markdown",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint js/**/*.js",
    "lint:fix": "eslint js/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,css,html,md}\"",
    "format:check": "prettier --check \"**/*.{js,css,html,md}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dcog989/Goat-Minify"
  },
  "keywords": [
    "minifier",
    "minify",
    "javascript",
    "css",
    "html",
    "code-tool",
    "web-app",
    "offline",
    "postcss"
  ],
  "author": "Chase McGoat",
  "license": "MIT",
  "dependencies": {
    "cssnano": "^7.1.2",
    "highlight.js": "^11.11.1",
    "html-minifier-terser": "^7.2.0",
    "postcss": "^8.5.6",
    "terser": "^5.44.1"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.10",
    "buffer": "^6.0.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "events": "^3.3.0",
    "globals": "^16.5.0",
    "jsdom": "^27.1.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vite": "^7.2.2",
    "vitest": "^4.0.10"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "printWidth": 120,
    "arrowParens": "always"
  }
}